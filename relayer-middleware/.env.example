# Active relayer to run
# Options: hermes, hermes-legacy, rly, rly-legacy
ACTIVE_RELAYER=hermes

# RPC Authentication (optional)
# These credentials will be injected into RPC URLs
RPC_USERNAME=
RPC_PASSWORD=

# Telemetry ports (optional, defaults shown)
HERMES_TELEMETRY_PORT=3001
HERMES_LEGACY_TELEMETRY_PORT=3002
RLY_METRICS_PORT=3003
RLY_LEGACY_METRICS_PORT=3004

# Log level (optional)
LOG_LEVEL=info